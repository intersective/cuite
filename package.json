{
  "name": "CUTIE",
  "version": "1.9.0",
  "author": "Practera",
  "homepage": "https://github.com/intersective/cutie",
  "localsegment": "<SEGMENT_KEY>",
  "scripts": {
    "ng": "ng",
    "start": "SEGMENT_KEY=$(jq -r .localsegment package.json); [[ ! -z \"$SEGMENT_KEY\" ]] && echo Local SEGMENT_KEY exist ||  SEGMENT_KEY='<SEGMENT_KEY>'; sed -i '' \"s/<SEGMENT_KEY>/$SEGMENT_KEY/g\" src/index.html; ng serve --port 4202",
    "dev": "npm run start",
    "build": "ng build --base-href './' && npx cap copy",
    "test": "ng test --watch=false",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "deploy": "ng build -c demo --base-href '/cutie/' && npx angular-cli-ghpages --dir=www",
    "sonar-test": "ng test --no-watch --code-coverage --browsers ChromeHeadless --karmaConfig=karma.conf.sonar.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "^11.2.14",
